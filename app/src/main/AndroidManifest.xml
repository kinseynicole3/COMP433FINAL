<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NotificationActivity"
            android:label="Set Up Notifications">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <receiver android:name=".AlertReceiver"/>
        <activity
            android:name=".EditStatusActivity"
            android:label="Edit Medication Status">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <activity
            android:name=".DelMedsActivity"
            android:label="Delete Medication">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <activity
            android:name=".CurrentMedsActivity"
            android:label="Current Medication">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <activity
            android:name=".MedHistoryActivity"
            android:label="Past Medication">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <activity
            android:name=".AddMedsActivity"
            android:label="Add Medication">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.test2.MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="MedControl"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>